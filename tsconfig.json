{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "noImplicitThis": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "baseUrl": "src",
      "skipLibCheck": true,
      "checkJs": false,
      "skipDefaultLibCheck": true,
      "target": "ES5"
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "target": "ES5"
    }
  }
}