{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "noImplicitThis": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "baseUrl": "src",
      "skipLibCheck": true,
      "checkJs": false,
      "skipDefaultLibCheck": true
  }
}