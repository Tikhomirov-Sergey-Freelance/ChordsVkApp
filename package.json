  "scripts": {
    "start:webpack": "webpack --mode=development --watch",
    "start:server": "nodemon index.js --watch dist/server.js",
    "start": "set NODE_ENV=development&&set HTTP=true&& npm-run-all --print-label --parallel start:*",
    "start-https": "set NODE_ENV=development&& npm-run-all --print-label --parallel start:*",
    "firebase-to-json": "ts-node ./src/server/migrations/firebase-to-json.ts",
    "firebase-to-mongo": "ts-node ./src/server/migrations/firebase-to-mongo.ts",
    "mongoload": "node ./src/server/database/ssh/mongoload",
    "mongopush": "node ./src/server/database/ssh/mongopush",
    "postinst": "npm install node_modules/@vkontakte/vkui/",
    "build": "webpack",
    "create-migration": "ts-node ./src/server/database/migration.ts add migration migr",
    "run-migration": "ts-node ./src/server/database/migration.ts up --migrate-all",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "test": "ts-mocha -p tsconfig.json ./src/**/*.test.ts",
    "generate": "set NODE_TLS_REJECT_UNAUTHORIZED=0&&graphql-codegen --config codegen.json"
  },
